{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": ["error", { "allow": ["log"] } ],
    "no-var": "off",
    "prefer-rest-params": "off",
    "prefer-spread": "off",
    "prefer-arrow-callback": "off",
    "func-names": ["error", "never"],
    "object-shorthand": "off",
  }
}
